{
    "name": "grmj_profile",
    "description": "Official Profile Landing Page for Gordon R Mullen Jr, Software Developer",
    "version": "1.1.2",
    "main": "server.js",
    "exports": {
        "import": "./dist/src/server.js",
        "require": "./dist/src/server.js"
    },
    "type": "module",
    "keywords": [
        "Gordon R Mullen Jr",
        "TypeScript",
        "nodemon",
        "ESLint",
        "Server",
        "express-handlebars",
        "Gordon's Resume",
        "Gordon's Profile Page",
        "prettier",
        "HooT™️ Webelistics®️, LLC",
        "Webelistics® CodeOperate©️ Software Development",
        "CodeOperate©️",
        "TypeScript/JavaScript/HTML/CSS",
        "jest TDD unit-testing",
        "Test Driven Development",
        "ts-node",
        "Web Components",
        "No Frameworks",
        "Front-end is Native Web Component Driven",
        "Internet Application",
        "server-side-rendered",
        "Shadow DOM",
        "ShadowDom",
        "Shadow-DOM",
        "HTML Template"
    ],
    "author": "Gordon R Mullen Jr <webelistics@gmail.com>",
    "license": "MIT",
    "scripts": {
        "test": "npx playwright test",
        "report": " npx playwright show-report",
        "build": "npx tsc -p .",
        "comp_css": "copyfiles --soft ./src/css/*.* ./dist && echo 'Successful copy of the main images folder'",
        "comp_images": "copyfiles --soft ./src/images/*.* ./dist/ && echo 'Successful copy of the main images folder'",
        "comp_static": "copyfiles --soft ./src/components/componentTools/**/**/*.* ./dist/ && echo 'Copied componentTools to ./dist/src/components/componentTools'",
        "start": "npm run dev",
        "dev": "cross-env NODE_ENV=development \"node ./dist/src/server.js\"",
        "temp": "cross-env NODE_ENV=development npm start",
        "prod": "cross-env NODE_ENV=production  node ./dist/src/server.js",
        "lint": "eslint . --fix",
        "pretty": "npx prettier --write src/server.ts",
        "appstate": "npx prettier --write src/server.ts && eslint . --fix && npm test",
        "pwtrace": "npx playwright show-trace trace.zip",
        "docs": "npx typedoc"
    },
    "dependencies": {
        "@types/uuid": "^8.3.4",
        "@types/vscode": "^1.74.0",
        "concurrently": "^7.6.0",
        "cors": "^2.8.5",
        "date-fns": "^2.29.3",
        "express": "^4.18.2",
        "express-handlebars": "^6.0.6",
        "mongoose": "^6.8.2",
        "node": "^19.3.0",
        "node-cache": "^5.1.2",
        "nodemon": "^2.0.20",
        "path": "^0.12.7",
        "serve-favicon": "^2.5.0",
        "typescript": "^4.9.5",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@playwright/test": "^1.30.0",
        "@tsconfig/node16": "^1.0.3",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.14",
        "@types/handlebars": "^4.1.0",
        "@types/handlebars-helpers": "^0.5.3",
        "@types/jest": "^29.4.0",
        "@types/morgan": "^1.9.3",
        "@types/node": "^18.11.9",
        "@types/serve-favicon": "^2.5.3",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.50.0",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-prettier": "^4.2.1",
        "gitignore": "^0.7.0",
        "jest": "^29.4.3",
        "morgan": "^1.10.0",
        "npmignore": "^0.3.0",
        "open": "^8.4.0",
        "prettier": "^2.8.1",
        "supertest": "^6.3.3",
        "ts-jest": "^29.0.5",
        "tsc-watch": "^6.0.0",
        "tslib": "^2.4.1",
        "typedoc": "^0.23.25",
        "typedoc-plugin-markdown": "^3.14.0"
    }
}
